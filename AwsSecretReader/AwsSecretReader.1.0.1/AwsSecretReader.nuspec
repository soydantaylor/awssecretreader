    <dependencies>
      <group targetFramework=".NETCoreApp2.0">
        <dependency id="AWSSDK.SimpleSystemsManagement" version="3.3.27.1" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="AWSSDK.SimpleSystemsManagement" version="3.3.27.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>